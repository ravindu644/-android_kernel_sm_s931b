&qupv3_se9_i2c_sda_active {
	config {
		drive-strength = <6>;
		bias-disable;
	};
};
&qupv3_se9_i2c_scl_active {
	config {
		drive-strength = <6>;
		bias-disable;
	};
};
&qupv3_se9_i2c_sleep {
